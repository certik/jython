# setup
type_name: slice
type_class: PySlice

# getsets
expose_getset: start getStart
expose_getset: stop getStop
expose_getset: step getStep

# exposed methods
expose_meth: indices o
expose_meth: __hash__
  throw Py.TypeError("unhashable type");
expose_wide_meth: __init__ -1 -1
  `vdeleg`(init);
  `void;
expose_new_mutable:
